generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Company {
  id                         String                      @id
  name                       String
  tickerSymbol               String?                     @map("ticker_symbol")
  exchangeSymbol             String?                     @map("exchange_symbol")
  uniqueSymbol               String?                     @map("unique_symbol")
  dateGenerated              String?                   @map("date_generated")
  securityName               String?                     @map("security_name")
  exchangeCountryIso         String?                     @map("exchange_country_iso")
  listingCurrencyIso         String?                     @map("listing_currency_iso")
  canonicalUrl               String?                     @map("canonical_url")
  uniqueSymbolSlug           String?                     @map("unique_symbol_slug")
  scoreId                    Int?                        @map("score_id")
  swsCompanyPriceClose       CompanyPriceClose[]
  swsCompanyScore            CompanyScore[]

  @@map("swsCompany")
}

model CompanyPriceClose {
  date            String
  company_id      String
  price           Float
  date_created    DateTime       @default(dbgenerated("CURRENT_TIMESTAMP"))
  swsCompany      Company        @relation(fields: [company_id], references: [id])

  @@id([date, company_id])

  @@map("swsCompanyPriceClose")
}

model CompanyScore {
  id                    String    @id
  company_id            String
  date_generated        String
  dividend              Int
  future                Int
  health                Int
  management            Int
  past                  Int
  value                 Int
  misc                  Int
  total                 Int
  sentence              String?
  swsCompany            Company   @relation(fields: [company_id], references: [id])

  @@map("swsCompanyScore")
}
